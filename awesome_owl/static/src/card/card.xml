<?xml version="1.0" encoding="utf-8" ?>
<templates xml:space="preserve">
    <t t-name="awesome_owl.card.card">
        <div class="card-body">
            <div class="card">
                <div class="card-header">
                    <h5 class="card-title">
                        <t t-esc="props.title"/>
                    </h5>
                </div>
                <div class="card-body">
                  <div class="card-text">
                    <t t-slot="default"/>
                </div>
              </div>
            </div>
        </div>
    </t>
</templates>